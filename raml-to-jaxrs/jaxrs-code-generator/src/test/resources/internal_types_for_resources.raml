types:
   funk:
     type: object
     properties:
        foo: string
        faa: number
/more:
    description: The search resource
    get:
        queryParameters:
            hello_dude:
                type: datetime
            fink:
                type: integer
        body:
            application/json:
                type: funk
                properties:
                   oneMore: string
        responses:
            200:
                body:
                    application/json:
                        type: string
    post:
        body:
            application/json:
                type: string
            application/xml:
                type: integer
        responses:
            201:
                body:
                    application/json:
                        type: string
